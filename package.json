{
  "name": "blue10-app",
  "version": "2.0.20",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start --dev-client --android",
    "ios": "expo start --dev-client --ios",
    "web": "expo start --web",
    "lint": "eslint App.tsx \"src/**/*.{js,jsx,ts,tsx}\" \"App.tsx\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "qa": "npm run prettier && npm run lint && npm run type-check",
    "type-check": "tsc --noEmit",
    "unimported": "npx unimported",
    "build:android": "eas build --local --profile development --platform android",
    "build:ios": "eas build --local --profile development-simulator --platform ios",
    "build:all": "npm run build:android && npm run build:ios",
    "build:deploy": "eas build --profile production --platform all --auto-submit",
    "eject:android": "expo prebuild --platform android",
    "eject:ios": "expo prebuild --platform ios"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@formatjs/intl-getcanonicallocales": "^2.5.5",
    "@formatjs/intl-locale": "^4.2.11",
    "@formatjs/intl-numberformat": "^8.15.4",
    "@formatjs/intl-pluralrules": "^5.4.4",
    "@react-native-material/core": "^1.3.7",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/stack": "^7.3.3",
    "@sentry/react-native": "~6.15.1",
    "@tanstack/react-query": "^5.80.7",
    "@types/react": "~19.1.8",
    "@types/semver": "^7.7.0",
    "axios": "^1.9.0",
    "core-js": "^3.43.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "expo": "^53.0.11",
    "expo-application": "~6.1.4",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.0",
    "expo-device": "~7.1.4",
    "expo-image-manipulator": "~13.1.7",
    "expo-localization": "~16.1.5",
    "expo-media-library": "~17.1.7",
    "expo-secure-store": "~14.2.3",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "i18next": "^25.2.1",
    "jwt-decode": "^4.0.0",
    "promise": "^8.3.0",
    "react": "19.0.0",
    "react-content-loader": "^7.0.2",
    "react-i18next": "^15.5.2",
    "react-native": "0.79.3",
    "react-native-document-scanner-plugin": "^1.0.1",
    "react-native-file-access": "^3.1.1",
    "react-native-gesture-handler": "~2.25.0",
    "react-native-images-to-pdf": "^0.2.1",
    "react-native-pager-view": "6.8.1",
    "react-native-safe-area-context": "5.4.1",
    "react-native-screens": "4.11.1",
    "react-native-svg": "15.12.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-webview": "13.15.0",
    "semver": "^7.7.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "unique-slug": "^5.0.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.57.0",
    "prettier": "^3.5.3"
  },
  "private": true
}
