{
  "name": "blue10-app",
  "version": "1.6.1",
  "main": "node_modules/expo/AppEntry.js",
  "prettier": "@wecreatesolutions/config-prettier",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start --dev-client --android",
    "ios": "expo start --dev-client --ios",
    "web": "expo start --web",
    "lint": "eslint App.tsx src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "qa": "npm run prettier && npm run lint && npm run type-check",
    "type-check": "tsc --noEmit",
    "unimported": "npx unimported",
    "build:android": "eas build --local --profile development --platform android",
    "build:ios": "eas build --local --profile development-simulator --platform ios",
    "build:all": "npm run build:android && npm run build:ios",
    "build:deploy": "eas build --profile production --platform all --auto-submit",
    "eject:android": "expo prebuild --platform android",
    "eject:ios": "expo prebuild --platform ios"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.0.1",
    "@formatjs/intl-getcanonicallocales": "^2.3.0",
    "@formatjs/intl-locale": "^3.4.0",
    "@formatjs/intl-numberformat": "^8.8.0",
    "@formatjs/intl-pluralrules": "^5.2.7",
    "@react-native-material/core": "^1.3.7",
    "@react-native-segmented-control/segmented-control": "2.4.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.15",
    "@sentry/react-native": "5.19.1",
    "@tanstack/react-query": "^5.24.1",
    "@types/react": "~18.2.45",
    "@types/semver": "^7.5.4",
    "@wecreatesolutions/config-eslint": "^1.0.1",
    "@wecreatesolutions/config-prettier": "^1.0.0",
    "axios": "^1.5.1",
    "core-js": "^3.36.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.3.1",
    "expo": "~50.0.8",
    "expo-application": "~5.8.3",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.9",
    "expo-device": "~5.9.3",
    "expo-image-manipulator": "~11.8.0",
    "expo-localization": "~14.8.3",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "i18next": "^23.6.0",
    "jwt-decode": "^4.0.0",
    "react": "18.2.0",
    "react-content-loader": "^6.2.1",
    "react-i18next": "^14.0.5",
    "react-native": "0.73.4",
    "react-native-document-scanner-plugin": "^0.9.1",
    "react-native-file-access": "^3.0.5",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-images-to-pdf": "^0.2.1",
    "react-native-pager-view": "6.2.3",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-webview": "13.6.4",
    "semver": "^7.5.4",
    "typescript": "^5.3.0",
    "unique-slug": "^4.0.0",
    "use-binding": "^2.0.1",
    "zustand": "^4.4.3",
    "expo-media-library": "~15.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2"
  },
  "private": true
}
