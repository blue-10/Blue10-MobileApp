{
  "name": "blue10-app",
  "version": "1.10.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start --dev-client --android",
    "ios": "expo start --dev-client --ios",
    "web": "expo start --web",
    "lint": "eslint App.tsx src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "qa": "npm run prettier && npm run lint && npm run type-check",
    "type-check": "tsc --noEmit",
    "unimported": "npx unimported",
    "build:android": "eas build --local --profile development --platform android",
    "build:ios": "eas build --local --profile development-simulator --platform ios",
    "build:all": "npm run build:android && npm run build:ios",
    "build:deploy": "eas build --profile production --platform all --auto-submit",
    "eject:android": "expo prebuild --platform android",
    "eject:ios": "expo prebuild --platform ios"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.0.1",
    "@formatjs/intl-getcanonicallocales": "^2.3.0",
    "@formatjs/intl-locale": "^4.0.0",
    "@formatjs/intl-numberformat": "^8.8.0",
    "@formatjs/intl-pluralrules": "^5.2.7",
    "@react-native-material/core": "^1.3.7",
    "@react-native-segmented-control/segmented-control": "2.5.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.4.0",
    "@sentry/react-native": "~5.22.0",
    "@tanstack/react-query": "^5.24.1",
    "@types/react": "~18.2.79",
    "@types/semver": "^7.5.4",
    "axios": "^1.5.1",
    "core-js": "^3.36.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.3.1",
    "expo": "^51.0.0",
    "expo-application": "~5.9.1",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.22",
    "expo-device": "~6.0.2",
    "expo-image-manipulator": "~12.0.5",
    "expo-localization": "~15.0.3",
    "expo-media-library": "~16.0.4",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "i18next": "^23.6.0",
    "jwt-decode": "^4.0.0",
    "promise": "^8.3.0",
    "react": "18.2.0",
    "react-content-loader": "^7.0.0",
    "react-i18next": "^15.0.1",
    "react-native": "0.74.5",
    "react-native-document-scanner-plugin": "^1.0.0",
    "react-native-file-access": "^3.0.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-images-to-pdf": "^0.2.1",
    "react-native-pager-view": "6.3.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-webview": "13.8.6",
    "semver": "^7.5.4",
    "typescript": "~5.3.3",
    "unique-slug": "^4.0.0",
    "use-binding": "^2.0.1",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^3.3.3"
  },
  "private": true
}