{
  "name": "blue10-app",
  "version": "2.1.4",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start --dev-client --android",
    "ios": "expo start --dev-client --ios",
    "web": "expo start --web",
    "lint": "eslint App.tsx \"src/**/*.{js,jsx,ts,tsx}\" \"App.tsx\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "qa": "npm run prettier && npm run lint && npm run type-check",
    "type-check": "tsc --noEmit",
    "unimported": "npx unimported",
    "build:android": "eas build --local --profile development --platform android",
    "build:ios": "eas build --local --profile development-simulator --platform ios",
    "build:all": "npm run build:android && npm run build:ios",
    "build:deploy": "eas build --profile production --platform all --auto-submit",
    "eject:android": "expo prebuild --platform android",
    "eject:ios": "expo prebuild --platform ios"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@formatjs/intl-getcanonicallocales": "^2.5.6",
    "@formatjs/intl-locale": "^4.2.12",
    "@formatjs/intl-numberformat": "^8.15.5",
    "@formatjs/intl-pluralrules": "^5.4.5",
    "@react-native-material/core": "^1.3.7",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/stack": "^7.4.9",
    "@sentry/react-native": "~6.22.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/react": "~19.2.0",
    "@types/semver": "^7.7.1",
    "axios": "^1.12.2",
    "core-js": "^3.45.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "expo": "^54.0.12",
    "expo-application": "~7.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.13",
    "expo-device": "~8.0.9",
    "expo-image-manipulator": "~14.0.7",
    "expo-localization": "~17.0.7",
    "expo-media-library": "~18.2.0",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "i18next": "^25.5.3",
    "jwt-decode": "^4.0.0",
    "promise": "^8.3.0",
    "react": "19.1.0",
    "react-content-loader": "^7.1.1",
    "react-i18next": "^16.0.0",
    "react-native": "0.81.4",
    "react-native-document-scanner-plugin": "^2.0.2",
    "react-native-file-access": "^3.2.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-pdf-from-image": "^0.3.5",
    "react-native-receive-sharing-intent": "^2.0.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "4.16.0",
    "react-native-svg": "15.13.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-webview": "13.16.0",
    "semver": "^7.7.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "unique-slug": "^5.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "babel-preset-expo": "^54.0.3",
    "eslint": "^9.37.0",
    "metro-react-native-babel-transformer": "^0.77.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  },
  "private": true
}
